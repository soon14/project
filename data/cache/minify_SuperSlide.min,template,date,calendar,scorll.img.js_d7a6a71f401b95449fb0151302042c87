if(this.even&&ele){$date=new Date(this.year,this.month,this.date);$this.year=$date.getFullYear();$this.month=$date.getMonth()+1;$this.date=$date.getDate();new Calendar().show(ele,id,url,"",$this.isChangeCss,$this.isWeb,lineid);ele.appendChild(ui);$this.change($this.year,$this.month,$this.date,1,idarr[2]);$(ele).find("a[id='subPrev']").eq(1).css("display","none");$(ele).find("a[id='subNext']").eq(0).css("display","none");$(ele).find("a[id='subPrev']").eq(0).attr("even","1");$(ele).find("a[id='subNext']").eq(1).css("even","1");$(ele).find("table").css({"float":"left"});}}
