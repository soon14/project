else{$.ajax({type:"post",url:"/public/ajax_set.php",data:"dopost=getPrice&hotelid="+idarr[0]+"&tid="+idarr[1],success:function(msg){if(!msg)
$("#zxyd").remove();Calendar_itinerary=eval("("+msg+")");if(Calendar_itinerary)
new Calendar().show(ele,id,url,iseven);}});}}
